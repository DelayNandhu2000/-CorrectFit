<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.CorrectFit" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Customize your light theme here. -->
        <!-- <item name="colorPrimary">@color/my_light_primary</item> -->
    </style>

    <style name="Theme.CorrectFit" parent="Base.Theme.CorrectFit" />

    <style name="SeekBarThumbStyle">
        <item name="android:height">@dimen/_25sdp</item>
        <item name="android:width">@dimen/_25sdp</item>
    </style>

    <style name="MainTextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="fontFamily">@font/interbold</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">@dimen/_16ssp</item>
        <item name="lineHeight">@dimen/_19ssp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#222222</item>
    </style>


    <style name="MainImage">
        <item name="android:layout_marginStart">@dimen/_16sdp</item>
        <item name="android:layout_marginEnd">@dimen/_16sdp</item>
        <item name="android:layout_marginTop">@dimen/_64sdp</item>
        <item name="android:gravity">center</item>
    </style>


    <style name="TypesOfProperties">
        <item name="fontFamily">@font/inter_semi_bold</item>
        <item name="android:layout_marginStart">@dimen/_16sdp</item>
        <item name="android:layout_marginEnd">@dimen/_16sdp</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">@dimen/_14ssp</item>
        <item name="lineHeight">@dimen/_17ssp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#222222</item>
    </style>


    <style name="TypesDescription">
        <item name="fontFamily">@font/inter_regular</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">@dimen/_12ssp</item>
        <item name="lineHeight">@dimen/_17sdp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">#858585</item>
        <item name="android:layout_marginTop">-2dp</item>
        <item name="android:layout_marginStart">8dp</item>
        <item name="android:layout_marginEnd">8dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">48dp</item>
        <item name="maxLines">2</item>
        <item name="android:padding">0dp</item>
    </style>


    <style name="WomanHoodSmallBox">
        <item name="android:layout_marginTop">@dimen/_12sdp</item>
        <item name="android:textColor">#FF566A</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingTop">@dimen/_6sdp</item>
        <item name="android:paddingBottom">@dimen/_6sdp</item>
        <item name="android:textSize">@dimen/_10ssp</item>
        <item name="fontFamily">@font/inter</item>
        <item name="paddingStart">@dimen/_16sdp</item>
        <item name="paddingEnd">@dimen/_16sdp</item>
        <item name="android:background">@drawable/womenhoodsmallboxstyle</item>
    </style>

    <style name="MainBoxText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">@dimen/_16sdp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textSize">@dimen/_10ssp</item>
        <item name="fontFamily">@font/inter</item>
        <item name="android:textColor">#222222</item>
    </style>

    <style name="MainBoxCircle">
        <item name="android:layout_width">@dimen/_20sdp</item>
        <item name="android:layout_height">@dimen/_20sdp</item>
        <item name="android:layout_marginEnd">@dimen/_16sdp</item>
        <item name="android:background">@drawable/womenhoodmainboxmaincircle</item>
    </style>

    <style name="MainBoxSmallCircle">
        <item name="android:layout_width">@dimen/_12sdp</item>
        <item name="android:layout_height">@dimen/_12sdp</item>
        <item name="android:visibility">gone</item>
        <item name="android:background">@drawable/womanhoodboxsmallcircle</item>
    </style>

    <style name="LayoutBox">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">@dimen/_25sdp</item>
        <item name="android:layout_marginEnd">@dimen/_25sdp</item>
    </style>

    <style name="MainBox">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/_36sdp</item>
        <item name="android:background">@drawable/womenhoodboxnotselectedstyle</item>
    </style>

    <style name="WomanHoodTitle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">@dimen/_14sdp</item>
        <item name="android:layout_marginEnd">@dimen/_14sdp</item>
        <item name="fontFamily">@font/inter</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">#222222</item>
        <item name="android:textSize">@dimen/_14ssp</item>
        <item name="android:textStyle">bold</item>

    </style>

    <style name="ButtonNext">
        <item name="android:layout_width">@dimen/_48sdp</item>
        <item name="android:layout_height">@dimen/_48sdp</item>
        <item name="android:layout_marginStart">4dp</item>
        <item name="background">@drawable/button_shape</item>
        <item name="android:drawableStart">@drawable/arrow_forward</item>
        <item name="paddingStart">@dimen/_16sdp</item>
    </style>

    <style name="ButtonBack">
        <item name="android:layout_width">@dimen/_48sdp</item>
        <item name="android:layout_height">@dimen/_48sdp</item>
        <item name="android:layout_marginEnd">24dp</item>
        <item name="background">@drawable/button_shape</item>
        <item name="android:drawableStart">@drawable/arrow_back</item>
        <item name="paddingStart">@dimen/_18sdp</item>
    </style>

    <style name="GuidLines">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="ButtonConstrain">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">@dimen/_55sdp</item>
        <item name="android:layout_marginStart">156dp</item>
        <item name="android:layout_marginEnd">156dp</item>
    </style>

    <style name="LinearLayoutStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">64dp</item>
        <item name="android:layout_marginBottom">64dp</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="SeekBar">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:max">20</item>
        <item name="android:progressBackgroundTint">#FF566A</item>
        <item name="android:progressTint">#FF566A</item>
        <item name="android:scrollbarSize">6dp</item>
        <item name="android:secondaryProgressTint">#FFE5E8</item>
        <item name="splitTrack">false</item>
        <item name="android:thumb">@drawable/thump</item>
    </style>

    <style name="typesLinearLayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">@dimen/_38sdp</item>
        <item name="android:layout_marginEnd">@dimen/_35sdp</item>
        <item name="android:layout_marginTop">@dimen/_4sdp</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:weightSum">3</item>
    </style>

    <style name="seekBarDescription">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textColor">#858585</item>
        <item name="android:textSize">@dimen/_12ssp</item>
        <item name="fontFamily">@font/inter</item>
    </style>

    <style name="phaseRecycle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_marginTop">52dp</item>
    </style>

    <style name="MeasureRecycle">
       <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">16dp</item>
        <item name="android:layout_marginTop">@dimen/_8sdp</item>
        <item name="android:layout_marginEnd">16dp</item>
    </style>

    <style name="EditText">
        <item name="android:layout_width">@dimen/_120sdp</item>
        <item name="android:layout_height">@dimen/_34sdp</item>
        <item name="android:gravity">center</item>
        <item name="android:inputType">number</item>
        <item name="android:maxLength">3</item>
        <item name="android:textColorHint">#FF566A</item>
        <item name="android:textSize">@dimen/_10ssp</item>
    </style>

    <style name="SizeWarnings">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="fontFamily">@font/inter_light</item>
        <item name="android:textColor">#FF566A</item>
        <item name="android:textSize">@dimen/_9sdp</item>
        <item name="android:visibility">gone</item>
    </style>

    <style name="logoDescription">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">50dp</item>
        <item name="android:layout_marginEnd">50dp</item>
        <item name="fontFamily">@font/inter_regular</item>
        <item name="android:textColor">#858585</item>
        <item name="android:textSize">@dimen/_12sdp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="perFitSize">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="fontFamily">@font/inter_semi_bold</item>
        <item name="android:gravity">center</item>
        <item name="lineHeight">@dimen/_18sdp</item>
        <item name="android:textColor">#222222</item>
        <item name="android:textSize">@dimen/_15ssp</item>
    </style>

    <style name = "perFitDescription">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">8dp</item>
        <item name="fontFamily">@font/inter_regular</item>
        <item name="android:textColor">#7E7E7E</item>
        <item name="android:textSize">@dimen/_12sdp</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="finalResultText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
