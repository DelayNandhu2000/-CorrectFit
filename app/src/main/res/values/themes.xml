        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">8dp</item>
        <item name="fontFamily">@font/inter_semi_bold</item>
        <item name="android:textColor">#FF566A</item>
        <item name="android:textSize">@dimen/_17ssp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="oldBraSize">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">4dp</item>
        <item name="fontFamily">@font/inter_semi_bold</item>
        <item name="android:textColor">#FF566A</item>
        <item name="android:textSize">@dimen/_17ssp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="oldSizeRecycler">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_marginStart">@dimen/_30sdp</item>
        <item name="android:layout_marginEnd">@dimen/_30sdp</item>
        <item name="android:layout_marginTop">4dp</item>
        <item name="layoutManager">androidx.recyclerview.widget.GridLayoutManager</item>
        <item name="spanCount">4</item>
    </style>

    <style name="brandRecycler">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_marginTop">@dimen/_20sdp</item>
        <item name="android:layout_marginStart">@dimen/_6sdp</item>
        <item name ="android:layout_marginEnd">@dimen/_6sdp</item>
        <item name="android:layout_marginBottom">@dimen/_20sdp</item>
        <item name="layoutManager">androidx.recyclerview.widget.GridLayoutManager</item>
        <item name="spanCount">2</item>


    </style>

</resources>